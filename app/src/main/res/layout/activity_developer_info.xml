<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    app:cardCornerRadius="16dp"
    tools:context=".Developer_info">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Team Motion Sense"
        android:textSize="27sp"
        android:textStyle="bold"
        android:fontFamily="sans-serif-black"
        app:layout_constraintBottom_toTopOf="@+id/ahadId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ahadId"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="111dp"
        android:layout_marginBottom="22dp"
        android:background="@drawable/custom_layout_back"
        app:layout_constraintBottom_toTopOf="@+id/ArnobId"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="20dp"
        android:clickable="true"
        android:foreground="?attr/selectableItemBackground"
        >


        <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/ahad_profile"
            android:layout_width="86dp"
            android:layout_height="86dp"
            android:layout_margin="10dp"
            android:src="@drawable/ahad_icon"
            app:civ_border_color="#FFFFFF"
            app:civ_border_width="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:includeFontPadding="false"
            android:text="Md Abdul Ahad"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/ahad_profile"
            app:layout_constraintTop_toTopOf="@id/ahad_profile" />

        <TextView
            android:id="@+id/email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:includeFontPadding="false"
            android:text="ID: 1822020015"
            android:padding="3dp"
            android:textColor="#94CDF9"

            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/ahad_profile"
            app:layout_constraintTop_toBottomOf="@id/name" />

        <TextView
            android:id="@+id/phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:includeFontPadding="false"
            android:text="@string/see"

            android:textColor="#F2B8B4"

            android:textSize="13sp"
            app:layout_constraintStart_toEndOf="@+id/ahad_profile"
            app:layout_constraintTop_toBottomOf="@id/email" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ArnobId"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/custom_layout_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ahadId"
        android:clickable="true"
        android:foreground="?attr/selectableItemBackground"
        >


        <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/arnob_profile"
            android:layout_width="86dp"
            android:layout_height="86dp"
            android:layout_margin="10dp"
            android:src="@drawable/dipto"
            app:civ_border_color="#FFFFFF"
            app:civ_border_width="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/name2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:includeFontPadding="false"
            android:text="Arnob Das Dipto"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"

            app:layout_constraintStart_toEndOf="@+id/arnob_profile"
            app:layout_constraintTop_toTopOf="@id/arnob_profile" />

        <TextView
            android:id="@+id/email2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:includeFontPadding="false"
            android:text="ID: 1832020022"
            android:textColor="#94CDF9"
            android:padding="5dp"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/arnob_profile"
            app:layout_constraintTop_toBottomOf="@id/name2" />

        <TextView
            android:id="@+id/phone2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:includeFontPadding="false"
            android:text="@string/see2"
            android:textColor="#F2B8B4"

            android:textSize="13sp"
            app:layout_constraintStart_toEndOf="@+id/arnob_profile"
            app:layout_constraintTop_toBottomOf="@id/email2" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/SaifurId"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/custom_layout_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ArnobId"
        android:clickable="true"
        android:foreground="?attr/selectableItemBackground"
        >


        <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/saifur_profile"
            android:layout_width="86dp"
            android:layout_height="86dp"
            android:layout_margin="10dp"

            android:src="@drawable/saiful"
            app:civ_border_color="#FFFFFF"
            app:civ_border_width="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/name3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="4dp"
            android:includeFontPadding="false"
            android:text="Saifur Rahman"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/saifur_profile"
            app:layout_constraintTop_toTopOf="@id/saifur_profile" />

        <TextView
            android:id="@+id/email3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:includeFontPadding="false"
            android:text="ID: 1822020012"
            android:padding="4dp"
            android:textColor="#94CDF9"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/saifur_profile"
            app:layout_constraintTop_toBottomOf="@id/name3" />

        <TextView
            android:id="@+id/phone3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"


            android:text="@string/see3"
            android:textColor="#F2B8B4"
            android:textSize="12sp"

            app:layout_constraintStart_toEndOf="@+id/saifur_profile"
            app:layout_constraintTop_toBottomOf="@id/email3" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>